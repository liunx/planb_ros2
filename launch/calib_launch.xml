<launch>
  <node pkg="planb_ros2" exec="calib_tool" output="screen">
    <param name="width" value="640"/>
    <param name="height" value="240"/>
    <param name="side" value="L"/>
  </node>
  <node pkg="planb_ros2" exec="brain" output="screen">
    <param name="bt_xml" value="$(find-pkg-share planb_ros2)/config/stereo_test.xml"/>
  </node>
</launch>
