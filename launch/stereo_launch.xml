<launch>
  <node pkg="planb_ros2" exec="stereoCV" output="screen">
    <param name="params" value="$(find-pkg-share planb_ros2)/config/depth_estimation_params.xml"/>
    <param name="maps" value="$(find-pkg-share planb_ros2)/config/stereo_rectify_maps.xml"/>
  </node>
  <node pkg="planb_ros2" exec="brain" output="screen">
    <param name="bt_xml" value="$(find-pkg-share planb_ros2)/config/stereo_test.xml"/>
  </node>
</launch>
